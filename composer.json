{
    "name": "fjarfs/src-service",
    "description": "Core HTTP request for SRC environment",
    "type": "library",
    "keywords": [
        "src",
        "sampoerna",
        "src-service"
    ],
    "require": {
        "php": "^7|^8"
    },
    "require-dev": {
        "laravel/laravel": "^5.6|^8.0",
        "laravel/lumen": "^5.8|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.4"
    },
    "autoload": {
        "psr-4": {
            "Fjarfs\\SrcService\\": "src"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fajar Sujito",
            "email": "fjarfs@gmail.com"
        }
    ],
    "scripts": {
        "cs": "phpcs --standard=PSR12 src/"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fjarfs\\SrcService\\SrcServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
